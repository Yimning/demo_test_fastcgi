3,修该了Make.build,添加修改了

CCP = arm-linux-g++

%.o : %.S
        echo "building $@"
        $(Q)$(CCP) -c $(CFLAGS)  $< -o $@
%.o : %.c
        echo "building $@"
        $(Q)$(CCP) -c $(CFLAGS) ${EXT_CFLAGS}  $< -o $@

$(DEPS):        $(LSRCS)
        echo "generating dependence file"
        @rm -f $@
        @for f in $(LSRCS); do \
        $(CCP) $(CFLAGS) ${EXT_CFLAGS} $(ALL_INCLUDES) -MM  $$f > $$f.tmp ; \
        sed 's,\($*\)\.o[ :]*,\1.o $@ : ,g' < $$f.tmp >> $@; \
        rm $$f.tmp; \
        done


